# expected_result_count: 170
SELECT DISTINCT
  ?qid
  ?orgLabel
  ?orgDescription
  ?type
  ?typeLabel
  ?country
WHERE {

  VALUES ?type {
    wd:Q748720 # EU institution (7)
    wd:Q112842785 # EU Interinstitutional Service (4)
    wd:Q112828372 # EU common foreign and security policy agency (3)
    wd:Q112828482 # Euratom agency (2)
    wd:Q64623553 # EU joint undertaking (10)
    wd:Q130417640 # EU delegation (144)
  }
  ?org wdt:P31 ?type .

  ?org wdt:P17 ?country .

  MINUS { ?org wdt:P576 [] }
  MINUS { ?org wdt:P1366 [] }

  BIND(REPLACE(STR(?org), "http://www.wikidata.org/entity/", "") AS ?qid)

  SERVICE wikibase:label { bd:serviceParam wikibase:language "en,mul" }
}
ORDER BY ?type ?orgLabel