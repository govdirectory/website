[
{{- range $i, $m := $ -}}
    {{- if contains $m.safeName "/" -}}
        {{- continue -}}
    {{- end -}}
    {{- if not $m.geoshape -}}
        {{- continue -}}
    {{- end -}}
    {{- if $i -}},{{- end -}}
    ["{{ $m.safeName}}", "{{ $m.geoshape }}", "{{ $m.name }}", "{{ $m.description }}"]
{{- end -}}
]
